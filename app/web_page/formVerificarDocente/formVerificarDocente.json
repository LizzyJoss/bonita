        "\t\t\tnombre: $data.docente.nombre !== undefined ? $data.docente.nombre : null,",
        "\t\t\tapellido: $data.docente.apellido !== undefined ? $data.docente.apellido : null,",
        "\t\t\tdireccion: $data.docente.direccion !== undefined ? $data.docente.direccion : null,",
